        <script type="text/javascript" >
            $(function () {
                //                $('#header').css({ height: $('#image').height(), width: '100%' });
                $('#tabs').css({ display: 'block', position: 'absolute', height: '99%', width: '99%' });
                $('#tabs').tabs({
                    activate: function (event, ui) {
                        $("#elementId").data("kendoWindow").center().close();
                        $("#id_Climat").data("kendoWindow").center().close();
                    }
                });
                // close icon: removing the tab on click
                $('#tabs').delegate("span.ui-icon-close", "click", function () {
                    var panelId = $(this).closest("li").remove().attr("aria-controls");
                    var myNode = document.getElementById(panelId);
                    while (myNode.firstChild) {
                        myNode.removeChild(myNode.firstChild);
                    }
                    $("#" + panelId).remove();
                    $('#tabs').tabs("refresh");
                });
            });
        </script>
        
