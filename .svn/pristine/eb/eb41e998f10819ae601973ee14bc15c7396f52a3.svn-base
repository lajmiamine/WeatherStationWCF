@model IEnumerable<WeatherStation.Models.MesuresPluviometrieClimat>

@{
    System.Globalization.CultureInfo customCulture = (System.Globalization.CultureInfo)System.Threading.Thread.CurrentThread.CurrentCulture.Clone();
    customCulture.NumberFormat.NumberDecimalSeparator = ".";
    System.Threading.Thread.CurrentThread.CurrentCulture = customCulture;
}          
<div id = "vertical2_Pluviometrie" style="height:95%; width: 100%;">
    <div id = "top-pane1">
        <ul id = "menu_date_pluv">
            <li id = "Horaire_pluv" style = "width:100px">Horaire</li>
            <li id = "Journaliere_pluv" style = "width:100px">Journaliére </li>
            <li id = "Hebdomadaire_pluv">Hebdomadaire </li>
            <li id = "mensuelle_pluv" style = "width:100px">Mensuelle</li>
            <li id = "periodique_pluv">Periodique</li>
        </ul>
        <ul id = "menu_reporting_pluv">
            <li onClick = "clickMonBoutonExcel();"><img id = "MonBoutonExcel" src="../../Content/images/excel.png" style="margin:0;" /></li>
            <li onClick = "clickMonBoutonPdf();"><img id = "MonBoutonPdf" src="../../Content/images/curve.png" /></li>
            <li><img src = "../../Content/images/pdf.jpeg" /></li>
            <li><img id = "refresh" src = "../../Content/images/refresh.png" /></li>
            <li><img id = "colorpickerPluv" onClick = "clickColorPicker();"></img></li>
        </ul>
        <div id = "window2" style = "display:none" >
            <center>
                <br /><br />
                    <strong><label for = "start">date debut:</label></strong><input id = "start" style = "width: 200px" />
                <br /><br />
                    <strong><label for = "end">date fin:</label></strong><input id = "end" style = "width: 200px" />
                <br /><br />
                <button type = "button" id = "check"  >Valider</button>
                <button type = "button" id = "Annuler_pluv"  >Annuler</button>
                <br /><br />
            </center>  
        </div>
    </div>
    <div id = "middle-pane1">
        <div id = "stock_chart_PLUV" style = "height: 95%"></div>
        <div id = "loading"></div>
    </div>
    <div id = "bottom-pane1">
        <div id = "grid_pluv" style = "height: 100%"></div>
    </div>
</div>

<script type = "text/javascript">

    $("#vertical2_Pluviometrie").kendoSplitter({
        orientation: "vertical",
        panes: [
            { collapsible: true, size: "6%" },
            { collapsible: true, size: "64%" },
            { collapsible: true, size: "30%" }
        ]
    });


   var list_pluv1 = [], list_pluv2 = [], orders = [], id = 1,
        list_pluv1_min = '@Model.Where(i => i.PluviometrieClimat != null).Min(j => j.PluviometrieClimat)',
        list_pluv1_max = '@Model.Where(i => i.PluviometrieClimat != null).Max(j => j.PluviometrieClimat)';

    @foreach (var mesure in Model)
    {
        @:list_pluv2.push('@mesure.DateMesure.ToString("yyyy/MM/dd HH:mm:ss")');
        @:if ('@mesure.PluviometrieClimat' != '') {
            @:list_pluv1.push(@mesure.PluviometrieClimat);
            @:orders.push({
                @:ID: id ++,
                @:OrderDate: '@mesure.DateMesure.ToString("yyyy/MM/dd HH:mm:ss")',
                @:Pluviometrie: '@mesure.PluviometrieClimat',
            @:});
        @:}
        @:else {
                @:list_pluv1.push(null);
        @:}
    }

    var list_seuil = [];

    $.ajax({
        url: '../../UserSession/SeuilsTemperature',
        type: 'Get',
        data: { idnoeudclimat: '@Model.ElementAt(0).IdParentNode'},
        datatype: 'json',
        async: false,
        success: function (res) {
            list_seuil.push(res.seuilInfTemperatureAir);
            list_seuil.push(res.seuilSuppTemperatureAir);
        }
    });

   

    function  Selection_Courbe(x)
    {
        var chart = $("#stock_chart_PLUV").data("kendoStockChart");
        var date =x.toString();
        var datefinale = new Date(Number(date.substring(0,4)),(Number(date.substring(5,7)))-1,Number(date.substring(8,10)),Number(date.substring(11,13)),Number(date.substring(14,16)),00);
        var select=0;
        for(var d=0;d<chart.series[0].data.length;d++)
        {
            var dattttttttttee = new Date(chart.series[0].data[d].x);
            if((datefinale.getFullYear()==dattttttttttee.getFullYear())&&(datefinale.getMonth()==dattttttttttee.getMonth())&&(datefinale.getDay()==dattttttttttee.getDay())&&(datefinale.getHours()==dattttttttttee.getHours())&&(datefinale.getMinutes()==dattttttttttee.getMinutes())&&(datefinale.getSeconds()==dattttttttttee.getSeconds())&&((chart.series[0].data[d].y)!=null))
            {
                select=d;
            }
        }

        chart.series[0].data[select].select();
        chart.series[0].markers.visible = true;
    }
    function clickColorPicker() {
        colorpicker.open();
    }

    var colorpicker = $("#colorpickerPluv").kendoColorPicker({
        change: function (e) {
            //kendoConsole.log("Change in picker #" + this.element.attr("id") + " :: " + e.value);
            var stockChart = $("#stock_chart_PLUV").data("kendoStockChart");
            stockChart.options.series[0].color = e.value;
            stockChart.options.valueAxis[0].color = e.value;
            stockChart.options.valueAxis[0].labels.color = e.value;
            stockChart.options.valueAxis[0].line.color = e.value;
            stockChart.options.valueAxis[0].title.color = e.value;
            stockChart.options.series[2].color = e.value;
            stockChart.refresh();
        },
        //  toolIcon: "k-foreColor"
    }).data("kendoColorPicker");
    function onChange(arg) {
        var selected = $.map(this.select(), function (item) {
           // Selection_Courbe($(item).text());
            return $(item).text();
        });
        kendoConsole.log("Selected: " + selected.length + " item(s), [" + selected.join(", ") + "]");
    }
     function onChangePluv(e) {
        
        var grid_pluv = $("#grid_pluv").data("kendoGrid");
        var rows_pluv = grid_pluv.select();
        var select_pluv = grid_pluv.items().index(rows_pluv); //alert("rows length: " + rows.length + "\nRows index: " + select + "\nRows uid: " + rows.uid);
        if (select_pluv != -1) {
            $(".k-grid-content").animate({
                scrollTop: ($('[data-uid = "' + grid_pluv.dataItem(rows_pluv).uid + '"] ').offset().top - $(".k-selectable").offset().top)
            }, 800);
                      
            var stockChartPluv = $("#stock_chart_PLUV").data("kendoStockChart");
            rows_pluv.each(function(index, row){
                var selectedItem = grid_pluv.dataItem(row);
//                alert("rows: " + selectedItem.ID + ", " + selectedItem.OrderDate + ", " + selectedItem.Temperature);
                var index_pluv = jQuery.inArray(selectedItem.Pluviometrie, stockChartPluv.options.series[0].data);
//                alert("index: " + index); alert(stockChart.options.series[0].data[index] + "\n" + selectedItem.Temperature);
                $(".k-tooltip").show();
            });
        }
    }
    $("#grid_pluv").kendoGrid({
        dataSource: {
            data: orders,
            pageSize: 500
        },
        height: '99%',
        change: onChangePluv,
        selectable: "multiple",
        scrollable: true,
        groupable: false,
        sortable: true,
        pageable: {
            refresh: true,
            pageSizes: true,
            buttonCount: 3
        },
        columns: [{
            field: "ID",
            title: "ID",
            width: 50,
            headerAttributes : {
                style: "text-align:center"
            }
        },{
            field: "OrderDate",
            title: "Date",
            format: "{0:dd/MM/yyyy HH:mm:ss}",
            width: 100,
            headerAttributes : {
                style: "text-align:center"
            }
        },
        {
            field: "Pluviometrie",
            title: "Pluviometrie(mm)",
            width: 100,
            headerAttributes : {
                style: "text-align:center"
            }
        }]
    });

   function selectRowPluv(Datee,pluv)
    {  
        var grid_pluv = $("#grid_pluv").data("kendoGrid"),
            view_pluv = grid_pluv.dataSource.view();
        var rows_pluv = $.grep(view_pluv, function(item){
        
            return item.Pluviometrie === pluv && String(item.OrderDate) === String(Datee);
        }).map(function(item) {  //alert('map: ' + item.uid);
            return grid_pluv.tbody.find(">[data-uid=" + item.uid + "]");
        });
        grid_pluv.clearSelection(); grid_pluv.select(rows_pluv);
       
    }

    $("#stock_chart_PLUV").kendoStockChart({
        title: {
            text: "Date : " + kendo.format('{0:dd/MM/yyyy}',new Date()),
            color: "#000000",
            font: "12px sans-serif",
        },
        dateField: null,
        series: [{
            name: "Pluviométrie",
            type: "line",
            style: "smooth",
            //field: "Temperature",
            data: list_pluv1,
            color:"#2f7ed8",
            markers: {
                visible : false,
            },
            tooltip: {
                visible: true,
                template: "Time=#= kendo.format('{0:dd/MM/yyyy HH:mm}',category) # ; #= series.name # = #= value # mm"
            },
        },{
            name: "Seuil",
        }],
        valueAxis: [{
            name: 'pluviométrie',
            data: list_pluv1,
            color:"#2f7ed8",
            labels: {
                format: "{0}"
            },
            min: Math.floor(list_pluv1_min /* - (max_pluv - min_pluv) * 0.1*/),
            max: Math.ceil(list_pluv1_max /*+ (max_pluv - min_pluv) * 0.1*/),
            title: {
                text: "pluviometrie (mm)",
                font: "12px sans-serif",
            },
            notes: {
                label:{
                    text :'seuil min',
                    position:'outside',
                    color: "#a0a700",
                },   
                visible: true,
                line: {
                    length:950,
                    color: "#a0a700",
                },
                data: [{
                    value: list_seuil[0],
                },
                {
                    value: list_seuil[1],
                    label: {
                        text: "seuil max"
                    }, 
                    position:'outside'
                }]
            },
            plotBands: [{
                from: -5,
                to: list_seuil[0],
                color: "#f5f5f5"
            },
            {        
                from: list_seuil[1],
                to: 70,
                color: "#f5f5f5"
            }]
        }],
        legend: {
            position: "bottom",
            visible: true,
            border: {
                width: 2,
                color: "grey",
                dashType: "solid"
            },
        },
        xAxis:{
            color:"#2f7ed8",
            labels: {
                template: "#= kendo.format('{0}',value/60) #"
            }
        },
        categoryAxis: {
            categories: list_pluv2,
            type: "date",
            baseUnit: "minutes",
            line:{
                color:"#2f7ed8",
            },
            labels: {
                visible: true,
                step: 120
            },
            majorGridLines: {
                visible: false,
            },
            majorTicks: {
                visible: true,
                size: 4,
                step: 60,
            }
        },
        navigator: {
            dateField: null,
            hint: {
                // hint template
                template: "From: #= from # To: #= to #",
                visible: false,
            },
            categoryAxis: {
                categories: list_pluv2,
                type: "date",
                autoBaseUnitSteps:{
                    minutes:[15] ,
                    hours:[2],
                }
            },
            select: {
                        from: list_pluv2[0],
                        to:list_pluv2[list_pluv2.length - 1],
            },
            series:{
                type: "area",
                area: {
                    line: {
                        style: "smooth"
                    }
                },
                data: list_pluv1,
                color: "#2f7ed8",
            },   
//                select: {
//                    from: list_pluv2[0],
//                    to: list_pluv2[(list_pluv2.length)-1]
//                }       
        },
        seriesClick: onSeriesClick_pluv,
        legendItemClick: onLegendItemClick,
    });

   function onSeriesClick_pluv(e) {
       
        /*kendoConsole.log//alert(kendo.format("Series click :: {0} ({1}): {2}",
                            e.series.name, e.category, e.value));*/
        var grid_chart = $("#grid_pluv").data("kendoGrid");
//                rowIndex = e.value,
//                row = grid.tbody.find(">tr:not(.k-grouping-row)").eq(rowIndex);
        // Retrieve all data from the DataSource
        var data_chart = grid_chart.dataSource.data();
        // Find those records that have OrderDate set to "test"
        // and return them in `res` array
        var res_chart = $.grep(data_chart, function (d) {
//            alert("d.OrderDate: " + d.OrderDate + "\ne.category: " + e.category);
//            alert("d.Temperature: " + d.Temperature + "\ne.value: " + e.value);
            return /*d.OrderDate == e.category &&*/ d.Pluviometrie == e.value;
        }); //alert(res);
//        if (row.length != 0) {
//            grid.scrollTop($(row).position().top);
//            $("#grid div.k-grid-content").scrollTop($(row).position().top); //scroll the content
//        }
//        grid.select(row);
        selectRowPluv(e.category, e.value);
    }
    function onLegendItemClick(e) { alert("onLegendItemClick\n" + e);
        var stockChart = $("#stock-chart").data("kendoStockChart");
        // kendoConsole.log(kendo.format("Legend item click :: {0}", e.text));
        // kendoConsole.log(kendo.format("Legend item click :: {0}", e.text));
        if(stockChart.options.valueAxis[0].notes.visible == false) {
            stockChart.options.valueAxis[0].notes.visible = true;
            stockChart.options.valueAxis[0].plotBands[0].color ="#f5f5f5";
            stockChart.options.valueAxis[0].plotBands[1].color ="#f5f5f5";
        }
        else{
            stockChart.options.valueAxis[0].notes.visible = false;
            stockChart.options.valueAxis[0].plotBands[0].color = "#ffffff";
            stockChart.options.valueAxis[0].plotBands[1].color = "#ffffff";
        }
    }
</script>

