            $("#tabs").children("ul:first").append("<li onclick='loadLeftSplitterWithParInfos(" + idParcel + ")'><a href='#par-" + idParcel + "'>" + nomParcel + "</a><span class='ui-icon ui-icon-close' role='presentation'>Remove Tab</span></li>");
